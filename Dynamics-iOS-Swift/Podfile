# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'Dynamics Swift Sample' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Dynamics Swift Sample
    pod 'BlackBerryDynamics', :podspec => 'https://software.download.blackberry.com/repository/framework/dynamics/ios/BlackBerryDynamics.podspec'
end

post_install do |installer|
  # Append configuration from default.xcconfig to configuration generated by Cocoapods
    system ("find Pods -name 'Pods-*.xcconfig' -exec sh -c 'cat \"`find . -name default.xcconfig`\" >> $1' -- {} \\;")
end
